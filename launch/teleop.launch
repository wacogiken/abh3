<launch>
    <node pkg="joy" type="joy_node" name="joy_node">
        <param name="dev" value="/dev/input/js0" />
        <param name="deadzone" value="0.1" />
        <param name="autorepeat_rate" value="20" />
    </node>

    <node pkg="abh3" name="abh3_joy" type="abh3_joy.py" output="screen">
        <rosparam>
            rate: 20
        </rosparam>
    </node>
</launch>
